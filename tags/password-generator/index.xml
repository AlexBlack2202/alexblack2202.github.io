      <description>&lt;p&gt;Tools sinh mật khẩu thông minh, tự động, tránh làm lộ mật khẩu&lt;/p&gt;&#xA;&lt;div class=&#34;container&#34;&gt;&#xA;&lt;div class = &#34;row&#34;&gt;&#xA;&lt;div class = &#34;col&#34;&gt;Chiều dài:&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&lt;select  class=&#34;form-select&#34;   name=&#34;passwords_length&#34; id=&#34;pass_length&#34;&gt;&#xA;  &lt;optgroup label=&#34;Mật khẩu yếu&#34; id=&#34;opg_weak&#34;&gt;&lt;/optgroup&gt;&#xA;  &lt;optgroup label=&#34;Mật khẩu mạnh&#34; id=&#34;opg_strong&#34;&gt;&lt;/optgroup&gt;&#xA;  &lt;optgroup label=&#34;Mật khẩu siêu mạnh&#34; id=&#34;opg_supperstrong&#34;&gt;&lt;/optgroup&gt;&#xA;&lt;/select&gt;&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&lt;button class=&#34;btn btn-primary&#34; onclick=&#34;gen_password();&#34;&gt;Generate Password &lt;/button&gt;&lt;/div&gt;&#xA;&lt;/div&gt;&#xA;&lt;div class = &#34;row&#34;&gt;&#xA;&lt;div class = &#34;col&#34;&gt;Ký tự thường:&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&#xA;&lt;div class=&#34;form-check&#34;&gt;&#xA; &lt;input class=&#34;form-check-input&#34; type=&#34;checkbox&#34; name=&#34;lower_case&#34; id=&#34;lower_case&#34; checked&gt;&#xA;  &lt;label class=&#34;form-check-label&#34; for=&#34;lower_case&#34;&gt;&#xA;   abcd&#xA;  &lt;/label&gt;&#xA;&lt;/div&gt;&#xA; &lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&lt;/div&gt;&#xA;&lt;/div&gt;&lt;div class = &#34;row&#34;&gt;&#xA;&lt;div class = &#34;col&#34;&gt;Ký tự hoa:&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&#xA;&lt;div class=&#34;form-check&#34;&gt;&#xA;&lt;input  class=&#34;form-check-input&#34;  type=&#34;checkbox&#34; name=&#34;upper_case&#34; id=&#34;upper_case&#34; checked&gt;&#xA;  &lt;label class=&#34;form-check-label&#34; for=&#34;upper_case&#34;&gt;&#xA;   ABCD&#xA;  &lt;/label&gt;&#xA;&lt;/div&gt;&#xA;&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&lt;/div&gt;&#xA;&lt;/div&gt;&#xA;&lt;div class = &#34;row&#34;&gt;&#xA;&lt;div class = &#34;col&#34;&gt;Ký số:&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&#xA;&lt;div class=&#34;form-check&#34;&gt;&#xA;&lt;input  class=&#34;form-check-input&#34;  type=&#34;checkbox&#34; name=&#34;numbers&#34; id=&#34;numbers&#34; checked=&#34;checked&#34;&gt;&#xA;  &lt;label class=&#34;form-check-label&#34; for=&#34;numbers&#34;&gt;&#xA;   1234&#xA;  &lt;/label&gt;&#xA;&lt;/div&gt;&#xA; &lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&lt;/div&gt;&#xA;&lt;/div&gt;&lt;div class = &#34;row&#34;&gt;&#xA;&lt;div class = &#34;col&#34;&gt;Ký tự đặc biệt&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&#xA;&lt;div class=&#34;form-check&#34;&gt;&#xA;&lt;input  class=&#34;form-check-input&#34;  type=&#34;checkbox&#34; name=&#34;symbols&#34; id=&#34;symbols&#34; checked&gt;&#xA;  &lt;label class=&#34;form-check-label&#34; for=&#34;symbols&#34;&gt;&#xA;    @#$!&#xA;  &lt;/label&gt;&#xA;&lt;/div&gt;&#xA;&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&lt;/div&gt;&#xA;&lt;/div&gt;&lt;div class = &#34;row&#34;&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&#xA;&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&lt;/div&gt;&#xA;&lt;/div&gt;&lt;div class = &#34;row&#34;&gt;&#xA;&lt;div class = &#34;col-2&#34;&gt;Pass của bạn:&lt;/div&gt;&#xA;&lt;div class = &#34;col-8&#34;&gt;&lt;textarea class=&#34;form-control&#34; style=&#34;min-height:200px;&#34; id=&#34;final_pass&#34;&gt;&lt;/textarea&gt;&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&lt;button class=&#34;btn btn-primary&#34; id=&#34;copy&#34; onclick=&#34;makeCopy();&#34;&gt;copy&lt;/button&gt;&lt;/div&gt;&#xA;&lt;/div&gt;&lt;div class = &#34;row&#34;&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&amp;nbsp;&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&amp;nbsp;&lt;/div&gt;&#xA;&lt;/div&gt;&lt;div class = &#34;row&#34;&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&amp;nbsp;&lt;/div&gt;&#xA;&lt;div class = &#34;col&#34;&gt;&amp;nbsp;&lt;/div&gt;&#xA;&lt;/div&gt;&#xA;&lt;/div&gt;&#xA;&lt;script type=&#34;text/javascript&#34;&gt;&#xA;&#xA;function makeCopy(){&#xA;     var textarea = document.getElementById(&#34;final_pass&#34;);&#xA;  textarea.select();&#xA;    document.execCommand(&#39;copy&#39;);&#xA;}&#xA;&#xA;function init(){&#xA;const weak_elem = document.getElementById(&#34;opg_weak&#34;);&#xA;for(var loop_index=6;loop_index&lt;16;loop_index++){&#xA;    var option = document.createElement(&#34;option&#34;);&#xA;        option.value = loop_index;&#xA;        option.text = loop_index;&#xA;        weak_elem.appendChild(option);&#xA;}&#xA;&#xA;const strong_elem = document.getElementById(&#34;opg_strong&#34;);&#xA;for(var loop_index=16;loop_index&lt;129;loop_index++){&#xA;    var option = document.createElement(&#34;option&#34;);&#xA;        option.value = loop_index;&#xA;        option.text = loop_index;&#xA;&#xA;        if(loop_index ===16){&#xA;            option.selected = true;&#xA;        }&#xA;        strong_elem.appendChild(option);&#xA;}&#xA;&#xA;var supper_strongs = [256,512,1204,2048,4096];&#xA;const super_strong_elem = document.getElementById(&#34;opg_supperstrong&#34;);&#xA;&#xA;for(const elem of supper_strongs){&#xA;var option = document.createElement(&#34;option&#34;);&#xA;        option.value = elem;&#xA;        option.text = elem;&#xA;        super_strong_elem.appendChild(option);&#xA;}&#xA;}&#xA;init();&#xA;&#xA;function gen_password(){&#xA;&#xA;    var random_length = document.getElementById(&#34;pass_length&#34;).value;&#xA;  var lower_case=   document.getElementById(&#34;lower_case&#34;).checked;&#xA;  var upper_case=   document.getElementById(&#34;upper_case&#34;).checked;&#xA;  var numbers=   document.getElementById(&#34;numbers&#34;).checked;&#xA;  var symbols=   document.getElementById(&#34;symbols&#34;).checked;&#xA;&#xA;&#xA;  if(! random_length){&#xA;      return;&#xA;  }&#xA;&#xA;&#xA;random_length = parseInt(random_length);&#xA;&#xA;const LOWER_CASE_CHARS = &#39;abcdefghijklmnopqrstuvwxyz&#39;.split(&#39;&#39;);&#xA;const UPPER_CASE_CHARS = LOWER_CASE_CHARS.map((x) =&gt; x.toUpperCase());&#xA;const SYMBOLS = &#39;!£$%^&amp;*()@~:;,./?{}=-_&#39;.split(&#39;&#39;);&#xA;const NUMBERS = &#39;1234567890&#39;.split(&#39;&#39;);&#xA;&#xA;function containsLowerCase(str) {&#xA;&#xA;    if (is_lower_case){&#xA; return LOWER_CASE_CHARS.some((x) =&gt; str.includes(x));&#xA;    }&#xA;    return true;&#xA;}&#xA;&#xA;function containsUpperCase(str) {&#xA;&#xA;    if (is_upper_case){&#xA;   return UPPER_CASE_CHARS.some((x) =&gt; str.includes(x));&#xA;    }&#xA;    return true;&#xA;}&#xA;&#xA;function containsSymbol(str) {&#xA;&#xA;    if (is_symbol){&#xA;  return SYMBOLS.some((x) =&gt; str.includes(x));&#xA;    }&#xA;    return true;&#xA;}&#xA;&#xA;function containsNumber(str) {&#xA;    if (is_number){&#xA;  return NUMBERS.some((x) =&gt; str.includes(x));&#xA;    }&#xA;    return true;&#xA;}&#xA;&#xA;var ramdom_list =[];&#xA;var is_lower_case=false;&#xA;var is_upper_case=false;&#xA;var is_number=false;&#xA;var is_symbol=false;&#xA;&#xA;&#xA;if(lower_case){&#xA;    is_lower_case=true;&#xA;    ramdom_list.push(...LOWER_CASE_CHARS);&#xA;}&#xA;&#xA;if(upper_case){&#xA;    is_upper_case=true;&#xA;    ramdom_list.push(...UPPER_CASE_CHARS);&#xA;}&#xA;&#xA;if(numbers){&#xA;    is_number=true;&#xA;    ramdom_list.push(...NUMBERS);&#xA;}&#xA;&#xA;if(symbols){&#xA;    is_symbol=true;&#xA;    ramdom_list.push(...SYMBOLS);&#xA;}&#xA;&#xA;function isValidPassword(password) {&#xA;  return containsLowerCase(password) &amp;&amp;  containsUpperCase(password) &amp;&amp;  containsSymbol(password) &amp;&amp;containsNumber(password) ;&#xA;}&#xA;&#xA;var arr_length = ramdom_list.length;&#xA;if(arr_length&lt;=0){&#xA;    return;&#xA;}&#xA;&#xA;// var arr_results = [];&#xA;// for (var iter=0; iter &lt; random_length;iter++ ){&#xA;// const randomElement = ramdom_list[Math.floor(Math.random() * arr_length)];&#xA;// arr_results.push(randomElement);&#xA;// }&#xA;&#xA; function generateStrongPassword(length = 16) {&#xA;  const buff = new Uint8Array(length);&#xA;&#xA;  let generatedPassword = &#39;&#39;;&#xA;&#xA;  do {&#xA;    window.crypto.getRandomValues(buff);&#xA;    generatedPassword = [...buff].map((x) =&gt; ramdom_list[x % arr_length]).join(&#39;&#39;);&#xA;  } while (!isValidPassword(generatedPassword));&#xA;&#xA;  return generatedPassword;&#xA;}&#xA;&#xA;document.getElementById(&#34;final_pass&#34;).value = generateStrongPassword(random_length);&#xA;}&#xA;&lt;/script&gt;&#xA;&lt;br/&gt;&#xA;&lt;h1 id=&#34;một-số-lưu-ý-khi-đặt-mật-khẩu&#34;&gt;Một số lưu ý khi đặt mật khẩu&lt;a class=&#34;anchor ms-1&#34; href=&#34;#một-số-lưu-ý-khi-đặt-mật-khẩu&#34;&gt;&lt;i class=&#34;fas fa-link&#34;&gt;&lt;/i&gt;&lt;/a&gt;&lt;/h1&gt;&#xA;&lt;p&gt;Để tránh cho mật khẩu bị tấn công bởi yếu hacker bằng kỹ thuật tấn công từ điển, tấn công từ điển, tấn công bằng social engineering, và giữ cho tài khoảng online của bạn được an toàn, bạn nên thực hiện những điều sau:&lt;/p&gt;</description>
